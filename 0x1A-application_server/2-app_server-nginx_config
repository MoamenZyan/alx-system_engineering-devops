server {
        listen 80 default_server;
location /hbnb_static { alias /data/web_static/current/;}
        listen [::]:80 default_server;


        add_header X-Served-By "482383-web-01";

        root /var/www/html;

        server_name _;

        location / {
                proxy_pass http://127.0.0.1:5000/airbnb-onepage/;
        }
}
